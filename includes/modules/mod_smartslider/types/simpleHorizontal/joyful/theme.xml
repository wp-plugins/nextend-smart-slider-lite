            <darkred>
                <outerborder>**|*|232323</outerborder>
                <bgcolor>ffffff</bgcolor>
                <tabbg>3e3e3e|*|bd161f</tabbg>
                <controllfont>{"Text":{"color":"FFFFFF"}}</controllfont>
                <headingfont>{"Heading":{"color":"bd161f"}, "Subheading":{"color":"3E3E3E"}}</headingfont>
                <paragraphfont>{"Link":{"color":"bd161f"}}</paragraphfont>
                <readmorefont>{"Small":{"color":"bd161f"}, "Big":{"color":"E4EAEE"}}</readmorefont>
                <captionfont>{"Tag":{"color":"FFFFFF"}}</captionfont>
