<?xml version="1.0" encoding="utf-8"?>
<config>
    <name>Simple horizontal Joyful theme</name>
    <params>
        <param name="skin" theme="simple_horizontal_joyful" type="offlajnskin" default="" size="5" label="Skin" description="">
            <darkblue>
                <outerborder>**|*|232323</outerborder>
                <bgcolor>ffffff</bgcolor>
                <tabbg>3e3e3e|*|188DD9</tabbg>
                <controllfont>{"Text":{"type":"Latin","family":"PT Sans Narrow","size":"20||px","color":"FFFFFF","bold":"1","italic":"0","underline":"0","align":"left","afont":"Arial, Helvetica||1","tshadow":"0||px|*|1||px|*|1||px|*|000000|*|1|*|","lineheight":"33px"}}</controllfont>
