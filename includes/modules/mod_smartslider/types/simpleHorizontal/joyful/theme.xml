            </darkblue>
            <darkgreen>
                <outerborder>**|*|232323</outerborder>
                <bgcolor>ffffff</bgcolor>
                <tabbg>3e3e3e|*|87b801</tabbg>
                <controllfont>{"Text":{"color":"FFFFFF"}}</controllfont>
                <headingfont>{"Heading":{"color":"87b801"}, "Subheading":{"color":"3E3E3E"}}</headingfont>
                <paragraphfont>{"Link":{"color":"87b801"}}</paragraphfont>
                <readmorefont>{"Small":{"color":"87b801"}, "Big":{"color":"E4EAEE"}}</readmorefont>
